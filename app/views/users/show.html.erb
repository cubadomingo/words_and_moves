<div class="container feed-container">
  <div class="col-lg-10 user-feed">
    <div class="col-md-12">
      <% if @user.preferred_regions.blank? %>
        <%= link_to "Find Regions", root_path, class: "btn btn-primary sign-in-button" %>
      <% end %>
      <% for item in @item_list %>
        <div class="row">
          <div class="col-lg-12 item">
            <%= render 'layouts/item_row', item: item %>
          </div>
        </div>
      <% end %>
    </div>
  </div>

<%= render 'layouts/sidenav' %>
</div>



